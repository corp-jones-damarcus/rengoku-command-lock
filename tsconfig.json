{
  "compilerOptions": {
    "module": "ESNext",
    "allowJs": true,
    "strict": true,
    "jsx": "preserve",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "noEmit": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": false
  },
  "include": ["./content/.vitepress/**/*"],
  "exclude": ["./content/.vitepress/cache/*"]
}
